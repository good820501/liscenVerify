<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="0dp"
            android:scaleType="fitXY"
            android:src="@drawable/home_top_bg" />

        <ImageButton
            android:id="@+id/imgBtnSel1"
            android:layout_width="64dip"
            android:layout_height="39dip"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="6dip"
            android:layout_marginTop="8dip"
            android:background="#00000000"
            android:padding="0dp"
            android:src="@drawable/btn_left_n" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dip"
            android:layout_toRightOf="@+id/imgBtnSel1"
            android:text="用户注册" />

        

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/workback"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:background="#ececec"
        android:padding="6dip"
        android:layout_marginTop="0dip"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginLeft="0dp" >

        

        <RelativeLayout
            android:id="@+id/InputPanel"
            android:layout_width="match_parent"
            android:layout_height="280dip"
            android:layout_alignLeft="@+id/toptext"
            
            android:background="@drawable/whileline"
            android:layout_marginTop="10dp" >
		<RelativeLayout
            android:id="@+id/username"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/sectopcaption"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                    
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="姓名:"
                    android:textColor="#595757"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/uname"
                    android:layout_width="wrap_content"
                    android:layout_height="40dip"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-5dp"
                    android:layout_toRightOf="@+id/textView3"
                    android:background="@null"
                     android:text=""
                    android:ems="10"
                     >

                <requestFocus />
            </EditText>
            </RelativeLayout>

            
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/userpass"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/username"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="-1dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout12"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                    
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="密码:"
                    android:textColor="#595757"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/upass"
                    android:layout_width="wrap_content"
                    android:layout_height="40dip"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-5dp"
                    android:layout_toRightOf="@+id/textView13"
                    android:background="@null"
                    android:ems="10"
                     android:text=""
                    android:inputType="textPassword" >

                <requestFocus />
            </EditText>
            </RelativeLayout>

            
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/userpassconfir"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/userpass"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="-1dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout12"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                    
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="确认密码:"
                    android:textColor="#595757"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/passcon"
                    android:layout_width="wrap_content"
                    android:layout_height="40dip"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-5dp"
                    android:layout_toRightOf="@+id/textView13"
                    android:background="@null"
                    android:ems="10"
                    android:text=""
                    android:inputType="textPassword" >

                <requestFocus />
            </EditText>
            </RelativeLayout>

            
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/phoneno"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/userpassconfir"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                   
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="手机号码:"
                    android:textColor="#595757"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/cellphone"
                    android:layout_width="110dip"
                    android:layout_height="40dip"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-8dp"
                    android:layout_toRightOf="@+id/textView3"
                    android:background="@null"
                    android:ems="15"
                    android:text=""
                    android:inputType="number|numberDecimal" >

                <requestFocus />
            </EditText>

                <ImageView
                    android:id="@+id/imageView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="53dp"
                    android:layout_toRightOf="@+id/cellphone"
                    android:src="@drawable/verify" />

            </RelativeLayout>

            
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/confirmcode"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/phoneno"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                    
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="验证码:"
                    android:textColor="#595757"
                    android:textSize="12sp" />

                <EditText
                    android:id="@+id/verifycode"
                    android:layout_width="wrap_content"
                    android:layout_height="40dip"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="-5dp"
                    android:layout_toRightOf="@+id/textView3"
                    android:background="@null"
                    android:ems="10"
                     android:text=""
                    android:inputType="number|numberDecimal" >

                <requestFocus />
            </EditText>
            </RelativeLayout>

            
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/sexytype"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/confirmcode"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                    
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="性别:"
                    android:textColor="#595757"
                    android:textSize="12sp" />
 		<RadioGroup
        android:id="@+id/gendergroup"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/textView3"
        android:orientation="horizontal" >
                               <RadioButton
                    android:id="@+id/radioMale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    
                     
                    android:text="男" />
               <RadioButton
                    android:id="@+id/radioFeMale"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dip"
                    
                    android:layout_toRightOf="@+id/radioMale"
                    android:text="女" />
               </RadioGroup>
            </RelativeLayout>

            
        </RelativeLayout>
         <RelativeLayout
            android:id="@+id/identifycode"
            android:layout_width="match_parent"
            android:layout_height="40dip"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/sexytype"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="0dip"
            android:background="@drawable/whileline" >

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="40dip"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dip"
                android:background="@drawable/whileline" >

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="80dip"
                    android:layout_height="match_parent"
                    
                    android:layout_marginLeft="10dip"
                    android:layout_marginTop="10dip"
                    android:text="身份证号:"
                    android:textColor="#595757"
                    android:textSize="12sp" />

               <EditText
                   android:id="@+id/idencode"
                   android:layout_width="wrap_content"
                   android:layout_height="40dip"
                   android:layout_alignParentTop="true"
                   android:layout_marginLeft="-5dp"
                   android:layout_toRightOf="@+id/textView3"
                   android:background="@null"
                    android:text=""
                   android:ems="10" >

                <requestFocus />
            </EditText>
            </RelativeLayout>

            
        </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/buttonlist"
            android:layout_width="match_parent"
            android:layout_height="80dip"
            android:layout_alignLeft="@+id/InputPanel"
            android:layout_below="@+id/InputPanel"
            android:layout_marginTop="23dp" >

            

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="192dip"
                android:layout_height="35dip"
                
                android:layout_marginLeft="55dip"
                
                android:scaleType="fitXY"
                android:src="@drawable/registesec" />

        </RelativeLayout>

        </RelativeLayout>

</RelativeLayout>
